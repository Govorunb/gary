{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "store:default",
    "websocket:default",
    "http:default",
    "log:default",
    "notification:default",
    "dialog:default",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "dialog:allow-message",
    "dialog:allow-open",
    "dialog:allow-save",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download-and-install",
    "process:allow-restart"
  ]
}